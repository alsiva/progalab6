<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="344.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lab.ui.LoginController">
    <Label fx:id="enterLabel" layoutX="155.0" layoutY="35.0" text="Enter panel">
        <font>
            <Font size="40.0" />
        </font>
    </Label>
   <VBox layoutX="160.0" layoutY="123.0" prefHeight="200.0" prefWidth="300.0">
      <children>
         <TextField fx:id="usernameField" promptText="username" onKeyPressed="#clearError" />
         <PasswordField fx:id="passwordField" promptText="password" onKeyPressed="#clearError" />
         <Label fx:id="errorLabel" prefHeight="34.0" prefWidth="307.0" textFill="#a12e2e" />
         <HBox alignment="BOTTOM_CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="25.0" prefWidth="300.0">
            <children>
                <Button fx:id="loginButton" mnemonicParsing="false" onAction="#logIn" prefHeight="25.0" prefWidth="150.0" text="Log in" HBox.hgrow="ALWAYS" />
                <Button fx:id="registerButton" mnemonicParsing="false" onAction="#register" prefHeight="25.0" prefWidth="150.0" text="Register" HBox.hgrow="ALWAYS" />
            </children>
         </HBox>
      </children>
   </VBox>
</AnchorPane>
