<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="lab.ui.ChangeLanguageController"
      prefHeight="400.0" prefWidth="600.0">
    <ComboBox fx:id="languageComboBox" prefWidth="150.0" promptText="Language" />
    <Button fx:id="changeLanguage" mnemonicParsing="false" onAction="#changeLanguage" prefHeight="25.0" prefWidth="132.0" text="Change Language" />
</HBox>
